.home-type{
  width: 100%;
  background-color: #fff;
  height: 300rpx;
  padding: 20rpx;
  box-sizing:border-box;
}
.home-type .ul{
  width: 100%;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  align-content: space-around;
  border: 1px dotted #327442;
}
.home-type .ul .li{
  width: 200rpx;
  height: 120rpx;
  display: flex;
  font-size: 24rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.home-type .ul .li image{
  width: 60rpx;
  height: 60rpx;
}